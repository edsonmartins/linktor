{
  "name": "@linktor/sdk",
  "version": "1.0.0",
  "description": "Official Linktor SDK for TypeScript/JavaScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "linktor",
    "chatbot",
    "messaging",
    "omnichannel",
    "whatsapp",
    "telegram",
    "facebook",
    "instagram",
    "sdk",
    "api"
  ],
  "author": "Linktor Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/linktor/linktor-sdk-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/linktor/linktor-sdk-typescript/issues"
  },
  "homepage": "https://docs.linktor.io/sdks/typescript",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
